{
    "env": {
      "browser": true,
      "es2020": true
    },
    "extends": [
      "airbnb-base",
      "plugin:vue/essential",
      "prettier"
      // "plugin:node/recommended"
    ],
    "plugins": ["prettier", "vue"],
    "rules": {
      "prettier/prettier": "warn",
      "no-unused-vars": "off",
      "no-unused-expressions": "off",
      "func-names": "off",
      "import/no-unresolved": "off",
      "no-param-reassign": "off"
    },
    "parserOptions": {
      "ecmaVersion": 11,
      "sourceType": "module"
    }
  }